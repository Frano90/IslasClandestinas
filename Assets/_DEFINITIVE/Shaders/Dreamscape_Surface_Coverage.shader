				#endif
				
				float2 uv_Normal = IN.ase_texcoord7.xy * _Normal_ST.xy + _Normal_ST.zw;
				float3 unpack76 = UnpackNormalScale( tex2D( _Normal, uv_Normal ), _NormalScale );
				unpack76.z = lerp( 1, unpack76.z, saturate(_NormalScale) );
				float3 tex2DNode76 = unpack76;
				float3 unpack79 = UnpackNormalScale( tex2D( _CoverageNormal, texCoord127 ), _CoverageNormalScale );
				unpack79.z = lerp( 1, unpack79.z, saturate(_CoverageNormalScale) );
				float3 tex2DNode79 = unpack79;
				#ifdef _BLENDNORMALSON_ON
				float3 staticSwitch80 = BlendNormal( tex2DNode76 , tex2DNode79 );
				#else
				float3 staticSwitch80 = tex2DNode79;
				#endif
				float3 temp_output_16_0_g4 = ( WorldPosition * 100.0 );
				float3 crossY18_g4 = cross( WorldNormal , ddy( temp_output_16_0_g4 ) );
				float3 worldDerivativeX2_g4 = ddx( temp_output_16_0_g4 );
				float dotResult6_g4 = dot( crossY18_g4 , worldDerivativeX2_g4 );
				float crossYDotWorldDerivX34_g4 = abs( dotResult6_g4 );
