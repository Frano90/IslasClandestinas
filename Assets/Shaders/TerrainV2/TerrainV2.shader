				float4 tex2DNode10 = tex2D( _Splatmap, uv_Splatmap );
				float AlphaChannel18 = tex2DNode10.a;
				float4 lerpResult103 = lerp( tex2DNode98 , tex2DNode24 , AlphaChannel18);
				float2 texCoord11 = IN.ase_texcoord7.xy * _TextureBTiling + float2( 0,0 );
				float4 tex2DNode23 = tex2D( _TextureBAlbedo, texCoord11 );
				float BlueChannel17 = tex2DNode10.b;
				float4 lerpResult91 = lerp( lerpResult103 , tex2DNode23 , BlueChannel17);
				float2 texCoord12 = IN.ase_texcoord7.xy * _TextureGTiling + float2( 0,0 );
				float4 tex2DNode20 = tex2D( _TextureGAlbedo, texCoord12 );
				float GreenChannel16 = tex2DNode10.g;
				float4 lerpResult101 = lerp( lerpResult91 , tex2DNode20 , GreenChannel16);
				float2 texCoord13 = IN.ase_texcoord7.xy * _TextureRTiling + float2( 0,0 );
				float4 tex2DNode26 = tex2D( _TextureRAlbedo, texCoord13 );
				float RedChannel15 = tex2DNode10.r;
				float4 lerpResult102 = lerp( lerpResult101 , tex2DNode26 , RedChannel15);
				
				float3 lerpResult108 = lerp( UnpackNormalScale( tex2D( _TextureBaseNormal, texCoord97 ), 1.0f ) , UnpackNormalScale( tex2D( _TextureANormal, texCoord14 ), 1.0f ) , AlphaChannel18);
				float3 lerpResult109 = lerp( lerpResult108 , UnpackNormalScale( tex2D( _TextureBNormal, texCoord11 ), 1.0f ) , BlueChannel17);
